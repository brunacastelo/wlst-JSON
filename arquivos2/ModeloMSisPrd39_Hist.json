{
	"template": "TEMPLATE_PLATAFORMAS",
	"weblogicPassword": "welcome1",
	"url": "localhost:7001",
	"user": "weblogic",
	"weblogic": "welcome1",
	"jmsModule": [
	{
		"name": "sismodule",
		"targets": [{
			"name": "sishistprd38a_sup",
			"type": "Server"
		},
		{
			"name": "sishistoryprd",
			"type": "Cluster"
		}],
		"connectionFactories": [{
			"name": "sis.HistoryJMSConnectionFactory",
			"jndiName": "sis.HistoryJMSConnectionFactory",
